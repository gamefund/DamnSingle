        .list-item-details span {
            height: 4vh;
            line-height: 4vh;
            font-size: 1.5rem;
        }

        .list-item-actions-icon {
            width: 5vw;
            height: 5vw;
            margin: auto 1vw;
            background-size: 100% 100%;
            background-repeat: no-repeat;
            margin: auto 1vw;
            display: inline-block;
        }

        .list-item-actions-number {
            font-size: 1.4rem;
            height: 6vw;
            line-height: 6vw;
        }

        .chose-page {
            width: 92.5%;
