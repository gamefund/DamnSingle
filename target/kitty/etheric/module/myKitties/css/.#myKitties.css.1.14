        .kitties-column-down {
            width: 100%;
            text-align: end;
        }

        .list-filter-select {
            border-radius: 5vw;
            height: 7.6vw;
            width: 100%;
            outline: none;
            border: 1px solid hotpink;
            padding: 0 0 0 8px;
            font-size: 1.2rem;
        }

