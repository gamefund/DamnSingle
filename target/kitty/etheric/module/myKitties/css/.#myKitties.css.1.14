        .list-item-container {
            width: 100%;
            height: 85vw;
            /*background-image: url('../image/kitty.png');*/
            /*background-size: 100% 100%;
            background-repeat: no-repeat*/
        }

        .list-item-container .kitty-item-photos {
            position: absolute;
            height: 85vw;
            width: 100%;
        }

        .list-item-status {
            position: relative;
            top: 8%;
            width: 40vw;
            margin: 0 auto;
        }

        .list-status-item {
            height: 7vw;
            line-height: 7vw;
            margin: 0 auto;
            border: 1px solid #fff;
            background-color: #fff;
            border-radius: 5vw;
            text-align: center;
            display: flex;
        }

        .list-status-itemIcon {
            width: 4vw;
            height: 4vw;
            margin: 1.5vw 1vw 1.5vw 1vw;
            /*background-image: url("../image/single.bmp");*/
            background-size: 100% 100%;
            background-repeat: no-repeat
        }

        .list-item-status .list-status-item span,
        .list-item-status .list-status-item small {
            font-size: 1.2rem;
        }

        .list-kitty-bottom {
            width: 100%;
            margin-bottom: 3.5vh;
            display: flex;
        }

        .list-item-actions {
            flex: 1;
            height: 6vw;
