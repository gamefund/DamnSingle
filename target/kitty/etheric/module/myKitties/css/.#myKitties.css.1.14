        .kitty-owner-main {
            position: absolute;
            top: 10vh;
            left: 0;
            right: 0;
            bottom: 0;
            overflow-x: hidden;
        }

        .kitty-owner-fixed {
            /*position: fixed;*/
            left: 0;
            right: 0;
            background-color: #fff;
            z-index: 1;
        }

        .kitty-owner-message {
            width: 85%;
            height: 16vh;
            display: flex;
            display: -webkit-flex;
            margin: auto;
        }

        .owner-headImage {
            width: 26vw;
            height: 26vw;
            background-image: url("../image/kitty.png");
            background-size: contain;
            background-repeat: no-repeat;
            margin: auto 2vw;
        }

        .owner-carte {
            margin: 2vh 0vw 1vh 6vw;
            width: 48vw;
        }

        .owner-name {
            margin: auto;
            font-size: 2.3rem;
            font-weight: bold;
            /*text-align: center;*/
            text-overflow: ellipsis;
            white-space: normal;
            overflow: hidden;
            height: 5vh;
            line-height: 5vh;
        }

        .owner-address {
            color: #9c9c9b;
            height: 4vh;
            line-height: 5vh;
            /*width: 27vw;*/
            /*border-bottom: 2px solid hotpink;*/
            /*margin-left: 1vw;*/
            font-size: 1.2rem;
        }

        .all-kitties-column {
            margin: 3vh auto;
            width: 85%;
            height: 7.6vw;
            line-height: 7.6vw;
            display: flex;
            display: -webkit-flex;
            justify-content: space-between;
        }

        .kitties-column-choise {
            display: flex;
            display: -webkit-flex;
            /*margin-left: 3vw;*/
            width: 50%;
