        }

        .owner-kitties-num {
            width: 85%;
            margin: 4vw auto;
            display: flex;
            display: -webkit-flex;
            height: 7.6vw;
            line-height: 7.6vw;
            color: #9c9c9b;
            border-bottom: 2px solid #dadad3;
            justify-content: space-between;
        }

        .all-kitties-style {
            font-size: 1.5rem;
            font-weight: bold;
            border-bottom: 2px solid hotpink;
            position: relative;
            top: 2px;
            margin-left: 3vw;
        }

        .owner-kitties-num .have-kitties {
            display: flex;
        }

        .owner-kitties-num .have-kitties div,
        .owner-kitties-num .have-kitties span {
            font-size: 1.4rem;
        }

        .all-kitties-message {
            width: 85%;
            position: absolute;
            left: 7.5vw;
            right: 7.5vw;
            /*top: 28vh;*/
            margin-bottom: 14vh;
        }

        .kitty-list-item {
            width: 100%;
